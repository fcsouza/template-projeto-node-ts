{
  "name": "primeiro-projeto-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "tsc && NODE_ENV=production node build/src/server.js",
    "dev:server": "ts-node-dev --inspect --transpileOnly --ignore-watch node_modules src/server.ts",
    "migration:create": "typeorm migration:create --dir src/migrations --name",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run",
    "test": "jest"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "express": "^4.17.1",
    "@sentry/node": "^5.23.0",
    "@sentry/tracing": "^5.23.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.14.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.0.0-pre.62",
    "typeorm": "^0.2.21",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.11",
    "@types/pg": "^7.11.2",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "faker": "^4.1.0",
    "jest": "^26.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4"
  }
}
